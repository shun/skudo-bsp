+	phy_write(phydev, 0x0e, 0x6);
+	phy_write(phydev, 0x0d, 0xc002);
+	phy_write(phydev, 0x0e, 0xffff);
+
+	phy_write(phydev, 0x0d, 0x2);
+	phy_write(phydev, 0x0e, 0x8);
+	phy_write(phydev, 0x0d, 0xc002);
